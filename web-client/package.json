{
  "name": "web-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "optimize-images": "node scripts/optimize-images.js",
    "build:prod": "npm run optimize-images && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\"",
    "test:e2e:pwa": "playwright test e2e/pwa/",
    "test:e2e:report": "playwright show-report",
    "docker:build": "scripts/docker-build.sh",
    "docker:build:dev": "scripts/docker-build.sh -f Dockerfile.dev -T development -t dev",
    "docker:run": "scripts/docker-run.sh run",
    "docker:dev": "scripts/docker-run.sh dev",
    "docker:stop": "scripts/docker-run.sh stop",
    "docker:logs": "scripts/docker-run.sh logs",
    "docker:clean": "scripts/docker-run.sh clean",
    "ci:test": "npm run type-check && npm run lint && npm run test:ci",
    "ci:build": "npm run ci:test && npm run build:prod",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.1.2",
    "@mui/x-data-grid": "^8.6.0",
    "@reduxjs/toolkit": "^2.8.2",
    "antd": "^5.26.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "framer-motion": "^12.19.2",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.3",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.5.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "terser": "^5.43.1",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-plugin-imagemin": "^0.6.1"
  }
}
